{
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fileDropEnabled": false,
        "fullscreen": false,
        "height": 1080,
        "minHeight": 600,
        "minWidth": 1200,
        "resizable": true,
        "title": "ChefDesktop",
        "width": 1920
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "bun run build",
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "identifier": "chef.cfrt.sh",
  "plugins": {},
  "productName": "ChefDesktop",
  "version": "0.0.0"
}
